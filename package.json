{
  "name": "secret-manager",
  "version": "1.0.10",
  "description": "secret-manager",
  "node_deb": {
    "entrypoints": {
      "daemon": "app.js --daemon"
    }
  },
  "main": "app.js",
  "scripts": {
    "develop": "nodemon app.ts",
    "start": "npm run build && npm run serve",
    "build": "npm run clean && tsc && npm run copy-files",
    "deb": "npm run build && cd dist && node-deb -- *",
    "copy-files": "cp .env ./dist && cp ./package.json ./dist",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "clean": "rimraf dist",
    "serve": "tsc-watch --outDir ./dist --onSuccess \"node ./dist/app.js\""
  },
  "author": "Beni",
  "license": "ISC",
  "dependencies": {
    "@types/crypto-js": "^3.1.46",
    "amazon-cognito-identity-js": "^4.3.1",
    "app-root-path": "^2.2.1",
    "aws-sdk": "^2.695.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cookie-parser": "^1.4.3",
    "crypto-js": "^4.0.0",
    "debug": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "hashset": "0.0.6",
    "helmet": "^3.21.2",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jasypt": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "method-override": "^2.3.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "node-deb": "^0.10.7",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.17.3",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "2.6.2",
    "socket.io": "^2.3.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^2.1.2",
    "tslint": "^5.8.0",
    "typescript": "^3.7.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^2.4.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.2",
    "@types/cors": "^2.8.5",
    "@types/del": "^4.0.0",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.0.22",
    "@types/lokijs": "^1.5.2",
    "@types/moment-timezone": "^0.5.12",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.5",
    "@types/nodemailer": "^6.4.0",
    "@types/request": "0.0.45",
    "sequelize-cli": "^5.5.1"
  }
}
